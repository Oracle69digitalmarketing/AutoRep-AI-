{
  "name": "autorep-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build:serverless": "tsc -p tsconfig.serverless.json --noEmitOnError false",
    "deploy": "sls deploy",
    "queue:dev": "ts-node-dev --respawn src/infra/sqs.worker.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.58.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.58.0",
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/client-pinpoint": "^3.58.0",
    "@aws-sdk/client-s3": "^3.58.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.58.0",
    "@aws-sdk/client-sqs": "^3.58.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "serverless-offline": "^14.4.0",
    "serverless-plugin-typescript": "^3.0.0-pre.82c9bc17"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^4.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
